<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Ingredient health benefit #<%= @ingredient_health_benefit.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/ingredient_health_benefits" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link ingredient_health_benefit start -->
      <div class="col">
        <a href="/ingredient_health_benefits/<%= @ingredient_health_benefit.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit ingredient health benefit
        </a>
      </div>
      <!-- Edit link ingredient_health_benefit end -->

      <!-- Delete link ingredient_health_benefit start -->
      <div class="col">
        <a href="/delete_ingredient_health_benefit/<%= @ingredient_health_benefit.id %>" class="btn btn-block btn-outline-secondary">
          Delete ingredient health benefit
        </a>
      </div>
      <!-- Delete link ingredient_health_benefit end -->
    </div>

    <dl>
      <dt>
        Ingredient
      </dt>
      <!-- Display ingredient_id start -->
      <dd>
        <%= @ingredient_health_benefit.ingredient_id %>
      </dd>
      <!-- Display ingredient_id end -->

      <dt>
        Health benefit
      </dt>
      <!-- Display health_benefit_id start -->
      <dd>
        <%= @ingredient_health_benefit.health_benefit_id %>
      </dd>
      <!-- Display health_benefit_id end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@ingredient_health_benefit.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@ingredient_health_benefit.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<!-- Show Page Customization end -->
