<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All health benefits
    </h1>

    <a href="/health_benefits/new" class="btn btn-block btn-outline-secondary">
      Add a new health benefit
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Name
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @health_benefits.each do |health_benefit| %>
      <tr>
        <!-- Display health_benefit.id start -->
        <td>
          <%= health_benefit.id %>
        </td>
        <!-- Display health_benefit.id end -->

        <!-- Display health_benefit.name start -->
        <td>
          <%= health_benefit.name %>
        </td>
        <!-- Display health_benefit.name end -->

        <!-- Display health_benefit.created_at start -->
        <td>
          <%= time_ago_in_words(health_benefit.created_at) %> ago
        </td>
        <!-- Display health_benefit.created_at end -->

        <!-- Display health_benefit.updated_at start -->
        <td>
          <%= time_ago_in_words(health_benefit.updated_at) %> ago
        </td>
        <!-- Display health_benefit.updated_at end -->

        <td>
          <a href="/health_benefits/<%= health_benefit.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
