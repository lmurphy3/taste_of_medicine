<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Health benefit #<%= @health_benefit.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/health_benefits" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/health_benefits/<%= @health_benefit.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit health benefit
        </a>
      </div>

      <div class="col">
        <a href="/delete_health_benefit/<%= @health_benefit.id %>" class="btn btn-block btn-outline-secondary">
          Delete health benefit
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Name
      </dt>
      <dd>
        <%= @health_benefit.name %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@health_benefit.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@health_benefit.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "health_benefit_templates/health_benefit_ingredient_health_benefits" %>
  </div>
</div>
