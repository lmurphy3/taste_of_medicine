<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Health benefit #<%= @health_benefit.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/health_benefits" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link health_benefit start -->
      <div class="col">
        <a href="/health_benefits/<%= @health_benefit.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit health benefit
        </a>
      </div>
      <!-- Edit link health_benefit end -->

      <!-- Delete link health_benefit start -->
      <div class="col">
        <a href="/delete_health_benefit/<%= @health_benefit.id %>" class="btn btn-block btn-outline-secondary">
          Delete health benefit
        </a>
      </div>
      <!-- Delete link health_benefit end -->
    </div>

    <dl>
      <dt>
        Name
      </dt>
      <!-- Display name start -->
      <dd>
        <%= @health_benefit.name %>
      </dd>
      <!-- Display name end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@health_benefit.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@health_benefit.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "health_benefit_templates/health_benefit_ingredient_health_benefits" %>
  </div>
</div>
<!-- Show Page Customization end -->
